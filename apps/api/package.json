{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "imports": {
    "#src/*.js": "./dist/*.js"
  },
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch './src/**' --watch '../../packages/features/dist' --onSuccess \"node --env-file=.env dist/index.js\"",
    "lint": "concurrently --raw pnpm:lint:*",
    "lint:eslint": "eslint src/",
    "lint:type-check": "tsc --noEmit",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/s3-request-presigner": "^3.722.0",
    "@clerk/fastify": "^2.1.4",
    "@fastify/auth": "^5.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@open-zero/database": "workspace:*",
    "@open-zero/features": "workspace:*",
    "@scalar/fastify-api-reference": "^1.25.99",
    "@sinclair/typebox": "^0.34.13",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "file-type": "^19.6.0",
    "openai": "^4.77.3",
    "playwright-chromium": "^1.49.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@open-zero/eslint-config-node": "workspace:*",
    "@open-zero/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "pino-pretty": "^13.0.0",
    "tsup": "8.3.0",
    "typescript": "~5.7.2"
  }
}
