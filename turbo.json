{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint:eslint": {
      "dependsOn": ["transit"]
    },
    "lint:type-check": {
      "dependsOn": ["transit"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": []
    },
    "dx": {
      "cache": false
    },
    "db:generate": {}
  },
  "ui": "tui"
}
